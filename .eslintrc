{
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "camelcase": 2,
    "comma-dangle": [2, "never"],
    "curly": 2,
    "default-case": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-fallthrough": 2,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-octal": 2,
    "no-redeclare": 2,
    "no-undef": 2,
    "no-unreachable": 2,
    "no-unused-vars": 2,
    "quotes": [2, "single"],
    "quote-props": [2, "as-needed"],
    "radix": 2,
    "semi": [2, "always"],
    "strict": [2, "global"]
  }
}
